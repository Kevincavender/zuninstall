#!/bin/sh
apt-get --yes --allow install python3-pip
apt-get --yes --allow install python3-scipy python3-matplotlib
apt-get --yes --allow install python3-django python3-bs4
apt-get --yes --allow install python3-reportlab python3-reportlab-accel
apt-get --yes --allow install imagemagick gifsicle
pip3 install pyeq3
git clone https://github.com/zunzun/zunzunsite3.git
python3 manage.py runserver
